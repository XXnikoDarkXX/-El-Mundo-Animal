<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nicolasfernandez.elmundoanimal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ElMundoAnimal">
        <activity android:name="actividades.Carga"></activity>
        <activity android:name="actividades.PurebaConsultaFirebase" />
        <activity android:name="actividades.Principal" />
        <activity android:name="actividades.LoginEmail" />
        <activity android:name="actividades.Registro" />
        <activity android:name="actividades.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>